<div style="padding: 3rem 1rem 0px;background: rgb(244, 246, 249); padding: 1rem;">
        <div class="slds-progress slds-progress_shade">
          <ol class="slds-progress__list" style="padding:0;"> 
            <li class="slds-progress__item  <% if(step>0){ %>slds-is-completed<%}%> <% if(step==0){ %>slds-is-active<%}%>">
              <button onclick="window.location.href='/?action=nextstep&step=0'" class="slds-button slds-progress__marker <% if(step>0){ %>slds-button_icon slds-progress__marker_icon<%}%>" title="Création de l'org">
                    <% if(step>0){%>
                <svg class="slds-button__icon" aria-hidden="true">
                  <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../dist/assets/icons/utility-sprite/svg/symbols.svg#success" />
                </svg>
                <%}%>
                <span class="slds-assistive-text">Création de l'org</span>
              </button>
            </li>
            <li class="slds-progress__item <% if(step>1){ %>slds-is-completed<%}%> <% if(step==1){ %>slds-is-active<%}%>">
              <button onclick="window.location.href='/?action=nextstep&step=1'" class="slds-button slds-progress__marker  <% if(step>1){ %>slds-button_icon slds-progress__marker_icon<%}%>" title="Création du modèle de donnée">
                    <% if(step>1){%>
                        <svg class="slds-button__icon" aria-hidden="true">
                          <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../dist/assets/icons/utility-sprite/svg/symbols.svg#success" />
                        </svg>
                        <%}%>
                <span class="slds-assistive-text">Step 2</span>
              </button>
            </li>
            <li class="slds-progress__item <% if(step>2){ %>slds-is-completed<%}%> <% if(step==2){ %>slds-is-active<%}%>">
              <button onclick="window.location.href='/?action=nextstep&step=2'" class="slds-button slds-progress__marker <% if(step>2){ %>slds-progress__marker slds-progress__marker_icon<%}%>" title="Chargement de la donnée">
                    <% if(step>2){%>
                        <svg class="slds-button__icon" aria-hidden="true">
                          <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../dist/assets/icons/utility-sprite/svg/symbols.svg#success" />
                        </svg>
                        <%}%>
                <span class="slds-assistive-text">Step 3</span>
              </button>
            </li>
            <li class="slds-progress__item <% if(step>3){ %>slds-is-completed<%}%> <% if(step==3){ %>slds-is-active<%}%>">
              <button onclick="window.location.href='/?action=nextstep&step=3'" class="slds-button slds-progress__marker <% if(step>3){ %>slds-progress__marker slds-progress__marker_icon<%}%>" title="Interface utilisateur">
                    <% if(step>3){%>
                        <svg class="slds-button__icon" aria-hidden="true">
                          <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../dist/assets/icons/utility-sprite/svg/symbols.svg#success" />
                        </svg>
                        <%}%>
                <span class="slds-assistive-text">Step 4 </span>
              </button>
            </li>
            <li class="slds-progress__item <% if(step>4){ %>slds-is-completed<%}%> <% if(step==4){ %>slds-is-active<%}%>">
              <button onclick="window.location.href='/?action=nextstep&step=4'" class="slds-button slds-progress__marker <% if(step>4){ %>slds-progress__marker slds-progress__marker_icon<%}%>" title="Automatisation">
                    <% if(step>4){%>
                        <svg class="slds-button__icon" aria-hidden="true">
                          <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../dist/assets/icons/utility-sprite/svg/symbols.svg#success" />
                        </svg>
                        <%}%>
                <span class="slds-assistive-text">Step 5 </span>
              </button>
            </li>
            <li class="slds-progress__item <% if(step>5){ %>slds-is-completed<%}%> <% if(step==5){ %>slds-is-active<%}%>">
                <button onclick="window.location.href='/?action=nextstep&step=5'" class="slds-button slds-progress__marker <% if(step>5){ %>slds-progress__marker slds-progress__marker_icon<%}%>" title="Rapports">
                      <% if(step>5){%>
                          <svg class="slds-button__icon" aria-hidden="true">
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../dist/assets/icons/utility-sprite/svg/symbols.svg#success" />
                          </svg>
                          <%}%>
                  <span class="slds-assistive-text">Step 6 </span>
                </button>
              </li>
              <li class="slds-progress__item <% if(step>6){ %>slds-is-completed<%}%> <% if(step==6){ %>slds-is-active<%}%>">
                <button onclick="window.location.href='/?action=nextstep&step=6'" class="slds-button slds-progress__marker <% if(step>6){ %>slds-progress__marker slds-progress__marker_icon<%}%>" title="Mobilité">
                      <% if(step>6){%>
                          <svg class="slds-button__icon" aria-hidden="true">
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../dist/assets/icons/utility-sprite/svg/symbols.svg#success" />
                          </svg>
                          <%}%>
                  <span class="slds-assistive-text">Step 7 </span>
                </button>
              </li>
              <li class="slds-progress__item <% if(step>7){ %>slds-is-completed<%}%> <% if(step==7){ %>slds-is-active<%}%>">
                <button onclick="window.location.href='/?action=nextstep&step=7'" class="slds-button slds-progress__marker <% if(step>7){ %>slds-progress__marker slds-progress__marker_icon<%}%>" title="Intégration">
                      <% if(step>7){%>
                          <svg class="slds-button__icon" aria-hidden="true">
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../dist/assets/icons/utility-sprite/svg/symbols.svg#success" />
                          </svg>
                          <%}%>
                  <span class="slds-assistive-text">Step 8 </span>
                </button>
              </li>
              <li class="slds-progress__item <% if(step>8){ %>slds-is-completed<%}%> <% if(step==8){ %>slds-is-active<%}%>">
                <button onclick="window.location.href='/?action=nextstep&step=8'" class="slds-button slds-progress__marker <% if(step>8){ %>slds-progress__marker slds-progress__marker_icon<%}%>" title="Développement">
                      <% if(step>8){%>
                          <svg class="slds-button__icon" aria-hidden="true">
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../dist/assets/icons/utility-sprite/svg/symbols.svg#success" />
                          </svg>
                          <%}%>
                  <span class="slds-assistive-text">Step 8 </span>
                </button>
              </li>
          </ol>
          <div class="slds-progress-bar slds-progress-bar_x-small" aria-valuemin="0" aria-valuemax="100" aria-valuenow="50" role="progressbar">
            <span class="slds-progress-bar__value" style="width: <%= (step/8)*100  %>%;">
              <span class="slds-assistive-text">Progress: 50%</span>
            </span>
          </div>
        </div>
      </div>